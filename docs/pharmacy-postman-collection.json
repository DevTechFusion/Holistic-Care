								"exec": [
									"if (pm.response.code === 201) {",
									"    const response = pm.response.json();",
									"    if (response.data && response.data.id) {",
									"        pm.environment.set('pharmacy_id', response.data.id);",
									"        console.log('Pharmacy ID saved: ' + response.data.id);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Accept",
								"value": "application/json"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"patient_name\": \"John Doe\",\n    \"date\": \"2025-09-11\",\n    \"phone_number\": \"1234567890\",\n    \"pharmacy_mr_number\": \"MR-2025-001\",\n    \"agent_id\": 1,\n    \"status\": \"completed\",\n    \"amount\": 150.75,\n    \"payment_mode\": \"cash\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/pharmacy",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"pharmacy"
							]
						}
					}
				},
				{
					"name": "Get All Pharmacy Records",
					"request": {
